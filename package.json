{
  "name": "vscode-spring-boot",
  "displayName": "Spring Boot Support",
  "description": "Spring Boot properties completion",
  "version": "0.0.7",
  "publisher": "ecmel",
  "license": "MIT",
  "homepage": "https://github.com/ecmel/vscode-spring-boot",
  "bugs": "https://github.com/ecmel/vscode-spring-boot/issues",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/ecmel/vscode-spring-boot.git"
  },
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "spring",
    "boot",
    "java",
    "properties"
  ],
  "activationEvents": [
    "onLanguage:ini"
  ],
  "main": "./out/src/extension",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "node-stream-zip": "1.3.4"
  }
}